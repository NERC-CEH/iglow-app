{
  "name": "iglow-app",
  "id": "uk.ac.ceh.iglow",
  "title": "iGlow App",
  "description": "iGlow mobile application.",
  "version": "1.0.0",
  "build": "1",
  "homepage": "https://www.firefliesandglow-worms.co.uk",
  "scripts": {
    "build": "cross-env NODE_ENV=production grunt",
    "build:update": "cross-env NODE_ENV=production grunt udpate",
    "build:dev": "grunt",	
    "build:species": "cd src/common/data && node species.js",
    "build:dev:update": "grunt update",
    "build:cordova": "npm run build && grunt cordova",
    "build:cordova:update": "grunt cordova:update",
    "build:cordova:android": "grunt cordova:android",
    "build:cordova:android:dev": "grunt exec:cordova_android_build_dev",
    "postinstall": "grunt",
    "start": "node other/server.js",
    "start-dev": "webpack-dev-server --config webpack.config.js --content-base dist/main --port 8000",
    "test": "karma start test/karma.conf.js",
    "test:coverage": "karma start test/karma.conf.js --reporters mocha,coverage",
    "test:watch": "karma start test/karma.conf.js --no-single-run",
    "test:sauce": "karma start test/karma.conf-sauce.js",
    "test:eslint": "mocha test/eslint-test.js",
    "test:acceptance": "grunt exec:cordova_build_android && ./node_modules/.bin/wdio test/acceptance/wdio.conf.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test && npm run test:eslint"
    }
  },
